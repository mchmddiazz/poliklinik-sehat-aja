datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/poliklinik_widyatama"
}

generator client {
  provider = "prisma-client-js"
}

model pasien {
  nomor_pendaftaran   String @id
  diagnosa            String?
  resep_obat          String?
  last_update         DateTime?
}

